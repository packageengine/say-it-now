{
  "name": "just-say-it",
  "version": "1.0.0",
  "description": "Multiple micro-response endpoints as a service (no/yes/maybe/joke/motivation/thank-you/apology)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "just-say-it": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "test": "npm run build && node test/index.test.js",
    "test:server": "npm run build && node test/server.test.js",
    "test:all": "npm run test && npm run test:server",
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build && npm run test"
  },
  "keywords": [
    "api",
    "microservice",
    "responses",
    "random",
    "no-as-a-service",
    "yes-as-a-service"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/packageengine/just-say-it.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  },
  "dependencies": {},
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}

